{
  "name": "aaronsaray/madmimi-api-php",
  "description": "A PHP library for rich, testable integration with MadMimi.",
  "keywords": ["madmimi", "api"],
  "homepage": "https://github.com/aaronsaray/madmimi-api-php",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron Saray",
      "email": "aaron@aaronsaray.com",
      "homepage": "http://aaronsaray.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "lib-curl": "~7.43",
    "symfony/yaml": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {"MadMimi\\": "src/MadMimi"}
  },
  "autoload-dev": {
    "psr-4": {"MadMimi\\tests\\": "tests/MadMimi"}
  }
}
